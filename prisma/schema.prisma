generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model user {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  bio       String
  email     String @unique
  firstName String
  image     String?
  lastName  String
  password  String
  username  String @unique
  verified  Boolean
}
